<!DOCTYPE html>
<html>
    <head>
        <link rel="stylesheet" href="/css/style.css" type="text/css">
    </head>
    <body>
        <h1 style="text-align:center;">Swift-MT300</h1>
        <hr width="50%">
        <span>
            <p style="padding-left:30%;padding-top:2%">One-to-One : &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; Matched = <%= matched %> &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; Mismatched = <%= mismatched %>
            <p style="padding-left:44%">Unmatched = <%= unmatched %> &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; Total = <%= total %></p>
        </span>
        <input class="search" list="search" name="search" placeholder="Service">
        <script>
            function CreateTableFromJSON() {
                var myBooks = //obj1;
                [
                    {
                        "Book ID": "1",
                        "Book Name": "Computer Architecture",
                        "Category": "Computers",
                        "Price": "125.60"
                    }
                ]
                var col = [];
                for (var i = 0; i < myBooks.length; i++) {
                    for (var key in myBooks[i]) {
                        if (col.indexOf(key) === -1) {
                            col.push(key);
                        }
                    }
                }
                var table = document.createElement("table");
                var tr = table.insertRow(-1);                   
                for (var i = 0; i < col.length; i++) {
                    var th = document.createElement("th");      
                    th.innerHTML = col[i];
                    tr.appendChild(th);
                }
                for (var i = 0; i < myBooks.length; i++) {
                    tr = table.insertRow(-1);
                    for (var j = 0; j < col.length; j++) {
                        var tabCell = tr.insertCell(-1);
                        tabCell.innerHTML = myBooks[i][col[j]];
                    }
                }
                var divContainer = document.getElementById("showData");
                divContainer.innerHTML = "";
                divContainer.appendChild(table);
            }
        </script>
    </body>
</html>
